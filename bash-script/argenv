log log=argenv $(basename $0) "$@"

for arg in "$@"; do
	if echo "$arg" | grep '=' >/dev/null 2>&1; then
		arg_name=$(echo $arg | sed 's =.*  ')
		arg_value=$(echo $arg | sed 's ^[^=]*=  ')
		eval ${arg_name}='${arg_value}'
		shift
	else
		break
	fi
done

set=${set-}
if test -n "$set"; then
	set $set
fi
